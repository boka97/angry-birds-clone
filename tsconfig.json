{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": ["DOM", "ESNext"],
        "allowJs": true,
        "sourceMap": true,
        "rootDir": "./",
        "removeComments": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    }
}
